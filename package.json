{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.9.0"
  },
  "scripts": {
    "start": "del db.json && hexo server",
    "live": "live-server --port=4000 public",
    "gen": "del db.json && cd source && git gc && cd .. && hexo generate && cd public && git add -A && git commit"
  },
  "dependencies": {
    "hexo": "git+https://github.com/hexojs/hexo.git#master",
    "hexo-deployer-git": "git+https://github.com/hexojs/hexo-deployer-git.git#master",
    "hexo-generator-archive": "git+https://github.com/hexojs/hexo-generator-archive.git#master",
    "hexo-generator-category": "git+https://github.com/hexojs/hexo-generator-category.git#master",
    "hexo-generator-feed": "git+https://github.com/hexojs/hexo-generator-feed.git#master",
    "hexo-generator-index": "git+https://github.com/hexojs/hexo-generator-index.git#master",
    "hexo-generator-tag": "git+https://github.com/hexojs/hexo-generator-tag.git#master",
    "hexo-inject": "git+https://github.com/hexojs/hexo-inject.git#master",
    "hexo-math": "git+https://github.com/hexojs/hexo-math.git#master",
    "hexo-renderer-ejs": "git+https://github.com/hexojs/hexo-renderer-ejs.git#master",
    "hexo-renderer-marked": "git+https://github.com/hexojs/hexo-renderer-marked.git#master",
    "hexo-renderer-stylus": "git+https://github.com/hexojs/hexo-renderer-stylus.git#master",
    "hexo-server": "git+https://github.com/hexojs/hexo-server.git#master"
  }
}